package_list=()
source ./preset-packages.conf
source ./extra-packages.conf

for i in ${package_list[@]}; do
        echo $i
        git clone https://github.com/rime/rime-$i.git package/rime/$i
        cd package/rime/$i
        rev=${i/\-/_}
        commit=$(eval echo \$${rev^^}_REV)
        git checkout $commit
        cd $SRCDIR
done

make all-bin
make DESTDIR="$PKGDIR" install
